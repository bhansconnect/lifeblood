{
  "name": "LifeBlood",
  "private": true,
  "description": "Analyze and track your blood test results over time. All data kept private and stored locally on your device.",
  "version": "0.1.0",
  "license": "BSD-3-Clause",
  "scripts": {
    "postinstall": "bsb -clean-world -make-world",
    "build": "bsb -make-world && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "re:start": "bsb -make-world -w",
    "serve": "gatsby serve",
    "clean": "bsb -clean-world && gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@glennsl/bs-json": "^5.0.2",
    "@jfrolich/gatsby-plugin-extract-schema": "0.0.4",
    "@jfrolich/gatsby-plugin-reason": "^0.1.4",
    "@reasonml-community/graphql-ppx": "^1.0.0-beta.21",
    "bs-css-emotion": "^1.2.0",
    "esm": "^3.2.25",
    "gatsby": "^2.24.14",
    "gatsby-plugin-manifest": "^2.4.21",
    "gatsby-plugin-netlify": "^2.3.11",
    "gatsby-plugin-offline": "^3.2.21",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-source-strapi": "0.0.12",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-markdown": "^4.3.1",
    "react-snap": "^1.23.0",
    "reason-gatsby": "^0.1.8",
    "reason-react": "^0.9.1"
  },
  "devDependencies": {
    "bs-platform": "^8.0.3",
    "dotenv": "^8.2.0",
    "emotion-server": "^10.0.27",
    "prettier": "2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bhansconnect/life-blood"
  },
  "bugs": {
    "url": "https://github.com/bhansconnect/life-blood/issues"
  }
}
